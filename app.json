{
  "expo": {
    "name": "app-tracer",
    "slug": "app-tracer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9072D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icons/adaptive-icon.png",
        "backgroundColor": "#D9072D"
      }
    },
    "web": {
      "favicon": "./src/assets/images/icons/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
